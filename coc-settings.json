{
    //java
    "java.enabled": true,
    "java.format.enabled": true,
    "java.completion.enabled": true,
    "java.project.encoding": "warning",
    "java.codeGeneration.useBlocks": true,
    "java.codeGeneration.generateComments": true,
    "java.autobuild.enabled": true,
    "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
    "java.signatureHelp.enabled": true,
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-17",
            "path": "C:/Program Files/Java/jdk-17"
        }
    ],
    //python
    "python.pythonPath": "C:/Users/alfgo/AppData/Local/Programs/Python/Python310/python.exe",
    "pyright.enable": true,
    "pyright.inlayHints.variableTypes": true,
    "pyright.organizeimports.provider": "pyright",
    "pyright.inlayHints.functionReturnTypes": true,
    "pyright.completion.snippetSupport": true,
    //javascript
    "javascript.referencesCodeLens.enabled": true,
    "javascript.implementationsCodeLens.enabled": true,
    "javascript.format.enable": true,
    "javascript.referencesCodeLens.showOnAllFunctions": true,
    "javascript.validate.enable": true,
    "javascript.autoClosingTags": true,
    "javascript.suggest.completeJSDocs": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "javascript.suggestionActions.enabled": true,
    "javascript.preferences.jsxAttributeCompletionStyle": "auto",
    "javascript.suggest.names": true,
    "javascript.suggest.autoImports": true,
    "javascript.showUnused": true,
    "javascript.preferences.quoteStyle": "double",
    "javascript.format.semicolons": "insert",
    "javascript.suggest.paths": true,
    "javascript.suggest.completeFunctionCalls": true,
    "javascript.inlayHints.variableTypes.enabled": true,
    "javascript.inlayHints.parameterTypes.enabled": true,
    "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
    "javascript.suggest.includeCompletionsForImportStatements": true,
    //typescript
    "typescript.referencesCodeLens.enabled": true,
    "typescript.referencesCodeLens.showOnAllFunctions": true,
    "typescript.validate.enable": true,
    "typescript.check.npmIsInstalled": true,
    "typescript.suggest.completeJSDocs": true,
    "typescript.implementationsCodeLens.enabled": true,
    "typescript.format.semicolons": "insert",
    "typescript.suggestionActions.enabled": true,
    "typescript.suggest.autoImports": true,
    "typescript.preferences.quoteStyle": "double",
    "typescript.inlayHints.variableTypes.enabled": true,
    "typescript.inlayHints.parameterTypes.enabled": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.preferences.jsxAttributeCompletionStyle": "auto",
    "typescript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": true,
    "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
    "typescript.suggest.includeCompletionsWithSnippetText": true,
    "typescript.suggest.includeAutomaticOptionalChainCompletions": true,
    //tsserver
    "tsserver.enable": true,
    "tsserver.useSyntaxServer": "auto",
    "tsserver.reportStyleChecksAsWarnings": true,
    "tsserver.experimental.enableProjectDiagnostics": true,
    //codelens
    "codeLens.enable": true,
    "codeLens.position": "top",
    "codeLens.separator": ">",
    "codeLens.subseparator": " | ",
    //css
    "css.format.enable": true,
    "css.hover.references": true,
    "css.hover.documentation": true,
    "css.format.braceStyle": "collapse",
    "css.completion.triggerPropertyValueCompletion": true,
    "css.completion.completePropertyWithSemicolon": true,
    //sql
    "sql.database": "mysql",
    "sql.lintOnOpen": true,
    "sql.lintOnSave": true,
    //json
    "json.enable": true,
    "json.format.enable": true,
    "json.schemaDownload.enable": true,
    "json.validate.enable": true,
    //htmldjango
    "htmldjango.hoverLang": "jinja2",
    "htmldjango.completion.enable": true,
    "htmldjango.builtin.pythonPath": "C:/Users/alfgo/AppData/Local/Programs/Python/Python310/python.exe",
    "htmldjango.djlint.enableLint": true,
    //html
    "html.suggest.html5": true,
    "html.validate.styles": true,
    "html.autoCreateQuotes": true,
    "html.autoClosingTags": true,
    "html.hover.references": true,
    "html.hover.documentation": true,
    //eslint
    "eslint.run": "onSave",
    "eslint.packageManager": "npm",
    "eslint.autoFixOnSave": true,
    "eslint.nodePath": "C:/Program Files/nodejs",
    "eslint.format.enable": true,
    "eslint.trace.server": "messages",
    "eslint.alwaysShowStatus": true,
    "eslint.codeAction.showDocumentation": {
        "enable": true
    },
    //emmet
    "emmet.includeLanguages": {
        "javascript": "javascriptreact",
        "svelte": "html"
    },
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.showAbbreviationSuggestions": true,
    //metals
    "metals.javaHome": "C:/Program Files/Java/jdk-17",
    "metals.statusBarEnabled": true,
    "metals.enable": true,
    "metals.sbtScript": "C:/Program Files (x86)/sbt/bin",
    "metals.showInferredType": true,
    "metals.showImplicitArguments": true,
    "metals.showImplicitConversionsAndClasses": true,
    "metals.superMethodLensesEnabled": true,
    //lua
    "Lua.codeLens.enable": true,
    "Lua.hint.enable": true,
    "Lua.semantic.enable": true,
    //git
    "git.enableGutters": true,
    "git.addGBlameToVirtualText": true,
    //coc
    "coc.preferences.formatOnSave": true,
    "coc.preferences.bracketEnterImprove": true,
    "coc.preferences.enableLinkedEditing": true,
    "coc.preferences.currentFunctionSymbolAutoUpdate": true
}
