{
    //java
    "java.enabled": true,
    "java.format.enabled": false,
    "java.completion.enabled": true,
    "java.codeGeneration.useBlocks": true,
    "java.codeGeneration.generateComments": true,
    "java.signatureHelp.enabled": true,
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-17",
            "path": "C:/Program Files/Java/jdk-17"
        }
    ],
    "java.project.referencedLibraries": [
        "lib/**/*.jar",
        "D:/Default/Proyectos/dependencias/mysql-connector-j-8.1.0"
    ],
    //python
    "pyright.enable": true,
    "pyright.inlayHints.variableTypes": true,
    "pyright.organizeimports.provider": "pyright",
    "pyright.inlayHints.functionReturnTypes": true,
    "pyright.completion.importSupport": true,
    "python.pythonPath": "C:/Python311/python.exe",
    //javascript
    "javascript.referencesCodeLens.enabled": true,
    "javascript.implementationsCodeLens.enabled": true,
    "javascript.format.enable": false,
    "javascript.referencesCodeLens.showOnAllFunctions": true,
    "javascript.validate.enable": true,
    "javascript.autoClosingTags": false,
    "javascript.suggest.completeJSDocs": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "javascript.suggestionActions.enabled": true,
    "javascript.preferences.jsxAttributeCompletionStyle": "auto",
    "javascript.suggest.names": true,
    "javascript.suggest.autoImports": true,
    "javascript.showUnused": true,
    "javascript.suggest.paths": true,
    "javascript.suggest.completeFunctionCalls": true,
    "javascript.inlayHints.variableTypes.enabled": true,
    "javascript.inlayHints.parameterTypes.enabled": true,
    "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
    "javascript.suggest.includeCompletionsForImportStatements": true,
    //typescript
    "typescript.format.enable": false,
    "typescript.referencesCodeLens.enabled": true,
    "typescript.referencesCodeLens.showOnAllFunctions": true,
    "typescript.validate.enable": true,
    "typescript.check.npmIsInstalled": true,
    "typescript.suggest.completeJSDocs": true,
    "typescript.implementationsCodeLens.enabled": true,
    "typescript.suggestionActions.enabled": true,
    "typescript.suggest.autoImports": true,
    "typescript.inlayHints.variableTypes.enabled": true,
    "typescript.inlayHints.parameterTypes.enabled": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.preferences.jsxAttributeCompletionStyle": "auto",
    "typescript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": true,
    "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
    "typescript.suggest.includeCompletionsWithSnippetText": true,
    "typescript.suggest.includeAutomaticOptionalChainCompletions": true,
    //tsserver
    "tsserver.enable": true,
    "tsserver.useSyntaxServer": "auto",
    "tsserver.reportStyleChecksAsWarnings": true,
    "tsserver.experimental.enableProjectDiagnostics": true,
    //codelens
    "codeLens.enable": true,
    "codeLens.position": "top",
    "codeLens.separator": ">",
    "codeLens.subseparator": " | ",
    //css
    "css.format.enable": false,
    "css.hover.references": true,
    "css.hover.documentation": true,
    "css.format.braceStyle": "collapse",
    "css.completion.triggerPropertyValueCompletion": true,
    "css.completion.completePropertyWithSemicolon": true,
    //c#
    "languageserver": {
        "csharp-ls": {
            "command": "csharp-ls",
            "filetypes": [
                "cs"
            ],
            "rootPatterns": [
                "*.csproj",
                ".vim/",
                ".git/",
                ".hg/"
            ]
        }
    },
    //sql
    "sql.database": "mysql",
    "sql.lintOnOpen": true,
    "sql.lintOnSave": true,
    //json
    "json.enable": true,
    "json.format.enable": false,
    "json.schemaDownload.enable": true,
    "json.validate.enable": true,
    //htmldjango
    "htmldjango.hoverLang": "jinja2",
    "htmldjango.completion.enable": true,
    "htmldjango.builtin.pythonPath": "C:/Python311/python.exe",
    "htmldjango.djlint.enableLint": true,
    //html
    "html.suggest.html5": true,
    "html.validate.styles": true,
    "html.autoCreateQuotes": true,
    "html.autoClosingTags": true,
    "html.hover.references": true,
    "html.hover.documentation": true,
    //vim
    "vimlsp.trace.server": "messages",
    "vimlsp.debug": false,
    "vimlsp.diagnostic.enable": true,
    "vimlsp.indexes.count": 5,
    //lua
    "Lua.codeLens.enable": true,
    "Lua.hint.enable": true,
    "Lua.semantic.enable": true,
    //git
    "git.enableGutters": true,
    "git.addGBlameToVirtualText": true,
    //coc
    "hover.target": "float",
    "coc.preferences.formatOnSave": false,
    "coc.preferences.formatOnType": false,
    "coc.preferences.bracketEnterImprove": true,
    "coc.preferences.enableLinkedEditing": true,
    "coc.preferences.currentFunctionSymbolAutoUpdate": true
}
